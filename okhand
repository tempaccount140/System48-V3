local aa=function(ab,bb)local cb=Instance.new(ab)for db,_c in pairs(bb)do cb[db]=_c end;return cb end local ba=aa("ScreenGui",{Parent=game:GetService("CoreGui"),Name="Splash"})local ca=aa("Frame",{Parent=ba,Name="Frame",Style="RobloxRound",Size=UDim2.new(0,668,0,120),Position=UDim2.new(0.5,-334,1,40)})local da=aa("ImageLabel",{Parent=ca,Name="welcome",BackgroundTransparency=1,Size=UDim2.new(1,0,0,67),Image="rbxassetid://700528822"})local _b=aa("ImageLabel",{Parent=ca,Name="filtering",BackgroundTransparency=1,Size=UDim2.new(0.75,0,0,34),Image="rbxassetid://"..(game:GetService("Workspace").FilteringEnabled and"700528180"or"700528461"),Position=UDim2.new(0.75,0,0,34)})wait(1)ca:TweenPosition(UDim2.new(0.5,-334,0.5,-60))wait(5)ca:TweenPosition(UDim2.new(0.5,-334,-1,-40))wait(1)ba:Destroy()
